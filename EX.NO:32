import matplotlib.pyplot as plt
import numpy as np

# Generate random data
np.random.seed(42)
x = np.random.rand(50)  # 50 random points for X
y = np.random.rand(50)  # 50 random points for Y

# Create scatter plot
plt.figure(figsize=(8,5))
plt.scatter(x, y, color='blue', marker='o', alpha=0.7)

# Add labels and title
plt.xlabel("X values")
plt.ylabel("Y values")
plt.title("Scatter Plot of Randomly Distributed X and Y")
plt.grid(True)

# Show plot
plt.show()
