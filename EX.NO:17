import pandas as pd

# Sample DataFrame
data = {
    "School_Code": ["S001", "S002", "S001", "S003", "S002", "S003", "S001"],
    "Student_Name": ["Alice", "Bob", "Charlie", "David", "Eva", "Frank", "Grace"],
    "Age": [14, 15, 13, 16, 14, 15, 14]
}

df = pd.DataFrame(data)

# Split by School_Code and get mean, min, max of Age
age_stats = df.groupby("School_Code")["Age"].agg(["mean", "min", "max"])

print(age_stats)
