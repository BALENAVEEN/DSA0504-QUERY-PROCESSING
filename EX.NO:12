import pandas as pd
import numpy as np

# Create DataFrame with random values
np.random.seed(3)
df = pd.DataFrame(
    np.random.randint(1, 100, size=(10, 4)),
    columns=["A", "B", "C", "D"]
)

# Apply background and font color styling
styled_df = df.style.set_properties(
    **{
        "background-color": "black",
        "color": "yellow"
    }
)

styled_df
