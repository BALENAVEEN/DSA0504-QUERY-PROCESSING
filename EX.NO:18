import pandas as pd

# Sample DataFrame
data = {
    "School_Code": ["S001", "S002", "S001", "S003", "S002", "S003", "S001"],
    "Class": ["10th", "10th", "9th", "10th", "9th", "9th", "10th"],
    "Student_Name": ["Alice", "Bob", "Charlie", "David", "Eva", "Frank", "Grace"],
    "Age": [14, 15, 13, 16, 14, 15, 14]
}

df = pd.DataFrame(data)

# Split DataFrame into groups based on School_Code and Class
grouped = df.groupby(["School_Code", "Class"])

# Check type of GroupBy object
print("Type of grouped object:", type(grouped))

# Display each group
for (school, cls), group in grouped:
    print(f"\nSchool_Code: {school}, Class: {cls}")
    print(group)
