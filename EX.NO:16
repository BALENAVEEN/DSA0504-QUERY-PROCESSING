import pandas as pd

# Sample DataFrame
data = {
    "School_Code": ["S001", "S002", "S001", "S003", "S002", "S003", "S001"],
    "Student_Name": ["Alice", "Bob", "Charlie", "David", "Eva", "Frank", "Grace"],
    "Grade": [85, 90, 78, 92, 88, 75, 80]
}

df = pd.DataFrame(data)

# Split DataFrame into groups based on School_Code
grouped = df.groupby("School_Code")

# Check type of GroupBy object
print("Type of grouped object:", type(grouped))

# Display each group
for school_code, group in grouped:
    print(f"\nSchool_Code: {school_code}")
    print(group)
